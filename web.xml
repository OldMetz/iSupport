<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

	<servlet>
	<servlet-name>ViewAllEstimatesServlet</servlet-name>
	<servlet-class>com.isupport.servlets.ViewAllEstimatesServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>NewEstimateServlet</servlet-name>
	<servlet-class>com.isupport.servlets.NewEstimateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ViewAllEstimatesServlet</servlet-name>
		<url-pattern>/allEstimates.jsp</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
	<servlet-name>NewEstimateServlet</servlet-name>
	<url-pattern>/newEstimate.jsp</url-pattern>
	</servlet-mapping>

</web-app>
